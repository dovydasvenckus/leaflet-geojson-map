{
  "name": "leaflet-geojson-map",
  "packageManager": "yarn@3.2.3",
  "alias": {
    "process": false,
    "buffer": false
  },
  "devDependencies": {
    "@parcel/config-default": "^2.7.0",
    "@parcel/core": "~2.7.0",
    "@types/leaflet": "1.7.11",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "buffer": "~6.0.3",
    "eslint": "^8.23.1",
    "husky": "~8.0.0",
    "lint-staged": "^13.0.3",
    "parcel": "~2.7.0",
    "pinst": "~3.0.0",
    "prettier": "2.7.1",
    "process": "~0.11.10",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "axios": "~0.27.2",
    "leaflet": "~1.8.0",
    "object-hash": "^3.0.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-leaflet": "~4.0.2"
  },
  "lint-staged": {
    "src/*": [
      "eslint --fix ",
      "prettier --write --ignore-unknown"
